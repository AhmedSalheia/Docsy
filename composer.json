{
    "name": "docsy/core",
    "version": "1.0.0",
    "description": "a package for automatically generating docs for laravel routes",
    "keywords": ["laravel", "documentation", "auto documentation", "api", "postman", "openapi"],
    "homepage": "https://github.com/yourusername/docsy",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Docsy\\": "src/",
            "Tests\\": "tests/"
        },
        "files": [
            "./helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Ahmed Salheia",
            "email": "AhmedSalheia.as@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/yaml": "^7.3",
        "guzzlehttp/guzzle": "^7.9"
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": []
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
